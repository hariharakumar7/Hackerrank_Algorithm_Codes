q=int(input())
while q>0:
    q-=1
    m=int(input())
    n=int(input())
    c=input().split()
    c=[int(x) for x in c]
    x=[]
    for i in range(len(c)):
        for j in range(i+1,len(c)):
            if c[i]+c[j]==m:
                print(min(i,j)+1,max(i,j)+1)
                break
            
   